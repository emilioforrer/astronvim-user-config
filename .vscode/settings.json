{
  "workbench.sideBar.location": "right",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "editor.tabSize": 4,
  "editor.wordWrap": "on",
  "editor.renderWhitespace": "all",
  "[elixir]": {
    "editor.defaultFormatter": "JakeBecker.elixir-ls"
  },
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "redhat.telemetry.enabled": false,
  "terminal.integrated.defaultProfile.linux": "bash",
  "editor.renderControlCharacters": true,
  "hediet.vscode-drawio.resizeImages": null,
  "window.zoomLevel": 1,
  "[python]": {
    "editor.formatOnType": true,
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "python.autoComplete.extraPaths": [],
  "python.analysis.extraPaths": [],
  "workbench.colorTheme": "Night Owl",
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1
  },
  // VIM
  "vim.easymotionDimBackground": true,
  "vim.handleKeys": {
    "<C-d>": true,
    "<C-s>": false,
    "<C-v>": false,
    "<C-z>": false
  },
  "vim.enableNeovim": true,
  "vim.startInInsertMode": true,
  "vim.neovimUseConfigFile": true,
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-a>"],
      "commands": [
        {
          "command": "editor.action.selectAll",
          "args": {
            "text": ""
          }
        }
      ]
    },
    {
      "before": ["<C-c>"],
      "commands": [
        {
          "command": "editor.action.clipboardCopyAction",
          "args": {
            "text": ""
          }
        }
      ]
    },
    {
      "before": ["<C-i>"],
      "commands": [
        {
          "command": "emojisense.quickEmoji",
          "args": {
            "text": ""
          }
        }
      ]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-a>"],
      "commands": [
        {
          "command": "editor.action.selectAll",
          "args": {
            "text": ""
          }
        }
      ]
    },
    {
      "before": ["<C-i>"],
      "commands": [
        {
          "command": "emojisense.quickEmoji",
          "args": {
            "text": ""
          }
        }
      ]
    },
    {
      "before": ["c"],
      "after": ["\"", "_", "c"]
    },
    {
      "before": ["C"],
      "after": ["\"", "_", "C"]
    },
    {
      "before": ["d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["D"],
      "after": ["\"", "_", "D"]
    },
    {
      "before": ["d", "d"],
      "after": ["\"", "_", "d", "d"]
    },
    {
      "before": ["<BS>"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["<space>"],
      "commands": ["whichkey.show"]
    },
    // NAVIGATION
    // switch b/w buffers
    {
      "before": ["<S-h>"],
      "commands": [":bprevious"]
    },
    {
      "before": ["<S-l>"],
      "commands": [":bnext"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-a>"],
      "commands": [
        {
          "command": "editor.action.selectAll",
          "args": {
            "text": ""
          }
        }
      ]
    },
    {
      "before": ["<C-i>"],
      "commands": [
        {
          "command": "emojisense.quickEmoji",
          "args": {
            "text": ""
          }
        }
      ]
    },
    {
      "before": ["<C-n>"],
      "commands": ["editor.action.addSelectionToNextFindMatch"]
    },
    {
      "before": ["<C-l>"],
      "commands": ["editor.action.selectHighlights"]
    },
    {
      "before": ["c"],
      "after": ["\"", "_", "c"]
    },
    {
      "before": ["C"],
      "after": ["\"", "_", "C"]
    },
    {
      "before": ["d"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["D"],
      "after": ["\"", "_", "D"]
    },
    {
      "before": ["d", "d"],
      "after": ["\"", "_", "d", "d"]
    },
    {
      "before": ["<BS>"],
      "after": ["\"", "_", "d"]
    },
    {
      "before": ["<space>"],
      "commands": ["whichkey.show"]
    },
    // Move selected lines while staying in visual mode
    {
      "before": ["J"],
      "commands": ["editor.action.moveLinesDownAction"]
    },
    {
      "before": ["K"],
      "commands": ["editor.action.moveLinesUpAction"]
    },
    {
      "before": ["g", "l"],
      "commands": ["editor.action.showDefinitionPreviewHover"]
    }
  ],
  "vim.visualModeKeyBindings": [],
  // WHICH KEY
  "whichkey.bindings": [
    {
      "key": "/",
      "name": "Comment line",
      "type": "command",
      "command": "editor.action.commentLine"
    },
    {
      "key": ";",
      "name": "Show commands",
      "type": "command",
      "command": "workbench.action.showCommands"
    },
    {
      "key": "c",
      "name": "Close active editor",
      "type": "command",
      "command": "workbench.action.closeActiveEditor"
    },
    {
      "key": "e",
      "name": "Toggle Explorer",
      "type": "command",
      "command": "workbench.action.toggleSidebarVisibility"
    },
    {
      "key": "m",
      "name": "Toggle minimap",
      "type": "command",
      "command": "editor.action.toggleMinimap"
    },
    {
      "key": "t",
      "name": "Toggle terminal",
      "type": "command",
      "command": "workbench.action.togglePanel"
    },
    {
      "key": "z",
      "name": "Toggle Zen mode",
      "type": "command",
      "command": "workbench.action.toggleZenMode"
    },
    {
      "key": "P",
      "name": "Paste selection to clipboard",
      "type": "command",
      "command": [
        "editor.action.selectAll",
        "editor.action.clipboardPasteAction"
      ]
    },
    {
      "key": "Y",
      "name": "Copy selection to clipboard",
      "type": "command",
      "command": "editor.action.clipboardCopyAction"
    },
    {
      "key": "n",
      "name": "New untitled file",
      "type": "command",
      "command": "workbench.action.files.newUntitledFile"
    },
    {
      "key": "l",
      "name": "+LSP",
      "type": "bindings",
      "bindings": [
        {
          "key": "f",
          "name": "Format Document",
          "type": "command",
          "command": "editor.action.formatDocument"
        },
        {
          "key": "d",
          "name": "Hover Diagnostics",
          "type": "command",
          "command": "editor.action.showDefinitionPreviewHover"
        },
        {
          "key": "a",
          "name": "LSP Code Action",
          "type": "command",
          "command": "editor.action.quickFix"
        },
        {
          "key": "s",
          "name": "Search symbols",
          "type": "command",
          "command": "workbench.action.quickOpen"
        }
      ]
    },
    {
      "key": "f",
      "name": "+Find",
      "type": "bindings",
      "bindings": [
        {
          "key": "f",
          "name": "Find files",
          "type": "command",
          "command": "workbench.action.quickOpen"
        },
        {
          "key": "o",
          "name": "Find History",
          "type": "command",
          "command": "workbench.action.quickOpen"
        }
      ]
    },
    {
      "key": "b",
      "name": "+Buffers",
      "type": "bindings",
      "bindings": [
        {
          "key": "|",
          "name": "Vertical split",
          "type": "command",
          "command": "workbench.action.splitEditor"
        },
        {
          "key": "\\",
          "name": "Horizontal split",
          "type": "command",
          "command": "workbench.action.splitEditorDown"
        },
        {
          "key": "d",
          "name": "Close active editor",
          "type": "command",
          "command": "workbench.action.closeActiveEditor"
        },
        {
          "key": "h",
          "name": "Move editor to left group",
          "type": "command",
          "command": "workbench.action.moveEditorToLeftGroup"
        },
        {
          "key": "j",
          "name": "Move editor to below group",
          "type": "command",
          "command": "workbench.action.moveEditorToBelowGroup"
        },
        {
          "key": "k",
          "name": "Move editor to above group",
          "type": "command",
          "command": "workbench.action.moveEditorToAboveGroup"
        },
        {
          "key": "l",
          "name": "Move editor to right group",
          "type": "command",
          "command": "workbench.action.moveEditorToRightGroup"
        },
        {
          "key": "f",
          "name": "Next editor",
          "type": "command",
          "command": "workbench.action.nextEditor"
        },
        {
          "key": "p",
          "name": "Previous editor",
          "type": "command",
          "command": "workbench.action.previousEditor"
        },
        {
          "key": "P",
          "name": "Paste clipboard to buffer",
          "type": "commands",
          "commands": [
            "editor.action.selectAll",
            "editor.action.clipboardPasteAction"
          ]
        },
        {
          "key": "b",
          "name": "Show all editor",
          "type": "command",
          "command": "workbench.action.showAllEditors"
        },
        {
          "key": "u",
          "name": "Reopen closed editor",
          "type": "command",
          "command": "workbench.action.reopenClosedEditor"
        },
        {
          "key": "l",
          "name": "Close all buffers to the left",
          "type": "command",
          "command": "workbench.action.closeEditorsInGroupToLeft"
        },
        {
          "key": "r",
          "name": "Close all buffers to the right",
          "type": "command",
          "command": "workbench.action.closeEditorsInGroupToRight"
        },
        {
          "key": "c",
          "name": "Close other editors",
          "type": "command",
          "command": "workbench.action.closeOtherEditors"
        },
        {
          "key": "Y",
          "name": "Copy buffer to clipboard",
          "type": "commands",
          "commands": [
            "editor.action.selectAll",
            "editor.action.clipboardCopyAction",
            "cancelSelection"
          ]
        }
      ]
    }
  ],
  "files.associations": {
    "*.excalidraw": "json",
    "*.excalidrawlib": "json",
    ".env*": "dotenv"
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "keyword.other.dotenv",
        "settings": {
          "foreground": "#FF000000"
        }
      }
    ]
  },
  "editor.inlineSuggest.suppressSuggestions": true,
  "yaml.schemas": {
    "file:///c%3A/Users/devem/.vscode/extensions/atlassian.atlascode-3.0.10/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
  },
  "remote.autoForwardPortsSource": "hybrid",
  "terminal.integrated.accessibleViewPreserveCursorPosition": true,
  "terminal.integrated.enableImages": true,
  "terminal.integrated.sendKeybindingsToShell": false,
  "editor.fontLigatures": true,
  "editor.fontFamily": "fira-code-nerd, FiraCode Nerd Font, Consolas, 'Courier New', monospace",
  "mermaid-editor.generate.scale": 1,
  "mermaid-editor.generate.quality": 5
}
